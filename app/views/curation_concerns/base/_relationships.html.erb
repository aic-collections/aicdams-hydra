<h2>Relationships</h2>

<%= render 'add_asset', presenter: presenter if presenter.citi_presenter? %>

<% if presenter.citi_presenter? && !presenter.viewable? %>
  <%= render 'shared/request_access', presenter: presenter %>
<% end %>

<% if presenter.has_relationships? %>
  <%= render 'relationships_table', presenter: presenter %>
<% else %>
  <p>No relationships found for this <%= presenter.human_readable_type %></p>
<% end %>
