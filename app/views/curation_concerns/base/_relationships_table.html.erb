<% if presenter.citi_presenter? %>
  <%= render_asset_relationship(presenter.representation_presenters, "Representations", true, presenter.preferred_representation_id) %>
  <%= render_asset_relationship(presenter.document_presenters, "Documentation") %>

<% else %>

  <%= render_citi_relationship(presenter.preferred_representation_presenters, "Preferred Representation Of") %>
  <%= render_citi_relationship(presenter.representation_presenters, "Representation Of") %>
  <%= render_citi_relationship(presenter.document_presenters, "Documentation For") %>
  <%= render_asset_relationship(presenter.attachment_presenters, "Attachment Of") %>
  <%= render_asset_relationship(presenter.attaching_presenters, "Attachments") %>

<% end %>
